@import "../../common/vendor.wxss";

.container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
  -webkit-align-content: flex-start;
  align-content: flex-start;
}
.cell {
  width: 220rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  text-align: center;
  font-size: 40rpx;
  color: #6D6D72;
  height: 240rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding: 0rpx;
  margin: 10rpx;
  border: 1rpx #D9D9D9 solid;
  background-color: #FFFFFF;
  border-radius: 10rpx 10rpx;
}
.cell-img {
  width: 200rpx;
  height: 200rpx;
  padding-top: 10rpx;
}
.cell-online {
  font-size: 24rpx;
  word-break: break-all;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  padding-left: 8rpx;
  padding-right: 8rpx;
  padding-bottom: 12rpx;
  text-align: left;
  width: 184rpx;
}