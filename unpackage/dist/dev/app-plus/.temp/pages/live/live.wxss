@import "../../common/vendor.wxss";

.container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
  -webkit-align-content: flex-start;
  align-content: flex-start;
}
.cell {
  width: 246rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  justify-content: center;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
  flex-direction: column;
  text-align: center;
  font-size: 40rpx;
  color: #6D6D72;
  height: 300rpx;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
  padding: 0rpx;
  margin: 0rpx;
  border-bottom: 1rpx solid #555;
}
.cell-img {
  border-radius: 50% 50%;
  height: 140rpx;
  width: 140rpx;
}
.cell-online {
  font-size: 30rpx;
}